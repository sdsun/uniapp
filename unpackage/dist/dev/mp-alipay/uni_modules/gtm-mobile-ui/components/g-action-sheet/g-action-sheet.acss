.g-action-sheet.data-v-40b05210 {
  background: #F5F5F5;
  border-radius: 24rpx 24rpx 0 0;
  text-align: center;
  overflow: hidden;
}
.g-action-sheet__title.data-v-40b05210 {
  font-size: 24rpx;
  color: #999;
  padding: 24rpx;
  background: #FFF;
}
.g-action-sheet__item.data-v-40b05210 {
  padding: 24rpx;
  border-top: 1px solid #F5F5F5;
  background: #FFF;
}
.g-action-sheet__item_text.data-v-40b05210 {
  color: #333;
  font-size: 28rpx;
}
.g-action-sheet__item_desc.data-v-40b05210 {
  color: #999;
  font-size: 24rpx;
  margin-top: 12rpx;
}
.g-action-sheet__item--disabled .g-action-sheet__item_text.data-v-40b05210,
.g-action-sheet__item--disabled .g-action-sheet__item_desc.data-v-40b05210 {
  color: #CCC;
}
.g-action-sheet__item--danger .g-action-sheet__item_text.data-v-40b05210 {
  color: #f56c6c;
}
.g-action-sheet__cancel.data-v-40b05210 {
  font-size: 28rpx;
  padding: 24rpx 0 46rpx 0;
  margin-top: 12rpx;
  background: #FFF;
}

